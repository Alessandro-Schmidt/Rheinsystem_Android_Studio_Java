<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"

    tools:context=".prototyp">

    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">

    <ImageButton
        android:id="@+id/btn_return"
        android:layout_width="213dp"
        android:layout_height="wrap_content"
        android:background="#00FFFFFF"
        android:backgroundTint="#00FFFFFF"
        android:clickable="true"
        android:contentDescription="@string/ignorea"
        android:focusable="true"
        android:minHeight="48dp"
        android:src="@drawable/tab_return"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="SpeakableTextPresentCheck,ImageContrastCheck,DuplicateSpeakableTextCheck" />

    <TextView
        android:id="@+id/title_hp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="28dp"
        android:layout_marginTop="36dp"
        android:text="@string/prototyp"
        android:textColor="#F9F9F9"
        android:textSize="25sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="TextContrastCheck" />

        <!--
            Left images:
        -->

        <ImageView
            android:id="@+id/imageView"
            android:layout_width="450dp"
            android:layout_height="600dp"
            android:layout_marginLeft="200dp"
            android:layout_marginTop="120dp"
            app:srcCompat="@drawable/img_proto_1"
            tools:layout_editor_absoluteX="137dp"
            tools:layout_editor_absoluteY="300dp" />

        <ImageView
            android:id="@+id/imageView_2"
            android:layout_width="450dp"
            android:layout_height="600dp"
            android:layout_marginLeft="200dp"
            android:layout_marginTop="730dp"
            app:srcCompat="@drawable/img_proto_3"
            tools:layout_editor_absoluteX="137dp"
            tools:layout_editor_absoluteY="300dp" />

        <ImageView
            android:id="@+id/imageView_5"
            android:layout_width="450dp"
            android:layout_height="450dp"
            android:layout_marginLeft="200dp"
            android:layout_marginTop="1340dp"
            app:srcCompat="@drawable/motor8"
            tools:layout_editor_absoluteX="137dp"
            tools:layout_editor_absoluteY="300dp" />


        <!--
            Right Images:
        -->

        <ImageView
            android:id="@+id/imageView3"
            android:layout_width="450dp"
            android:layout_height="600dp"
            android:layout_marginTop="120dp"
            android:layout_marginLeft="700sp"
            app:srcCompat="@drawable/img_proto_2" />

        <ImageView
            android:id="@+id/imageView_4"
            android:layout_width="450dp"
            android:layout_height="600dp"
            android:layout_marginTop="730dp"
            android:layout_marginLeft="700sp"
            app:srcCompat="@drawable/img_proto_4"/>

        <ImageView
            android:id="@+id/imageView_6"
            android:layout_width="450dp"
            android:layout_height="600dp"
            android:layout_marginTop="1340dp"
            android:layout_marginLeft="700sp"
            app:srcCompat="@drawable/ablohnungform"/>




    </RelativeLayout>
</ScrollView>